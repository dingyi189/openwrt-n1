
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option upd_privateip '0'

config service 'oray'
	option service_name 'oray.com'
	option enabled '1'
	option lookup_host 'xxxx.xxxx.xxxx'
	option domain 'xxxx.xxxx.xxxx'
	option username 'xxxxxxxx'
	option password 'xxxxxxxx'

